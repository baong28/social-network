  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Tạo một mạng giả lập với Power Law\n",
    "G = nx.barabasi_albert_graph(1000,2) # Mạng scale free\n",
    "degrees = [G.degree(n) for n in G.nodes()]\n",
    "degrees"
   ]
